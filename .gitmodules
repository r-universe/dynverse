[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "anndata"]
	path = anndata
	url = https://github.com/dynverse/anndata
	shallow = true
	registered = true
[submodule "babelwhale"]
	path = babelwhale
	url = https://github.com/dynverse/babelwhale
	shallow = true
	registered = true
[submodule "dynutils"]
	path = dynutils
	url = https://github.com/dynverse/dynutils
	shallow = true
	branch = master
	registered = false
